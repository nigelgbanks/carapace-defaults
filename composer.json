{
  "name": "islandora/carapace-defaults",
  "description": "A set of reasonable default configurations and layouts for Islandora 8 using the Carapace theme",
  "type": "drupal-module",
  "keywords": [
    "drupal",
    "carapace",
    "islandora"
  ],
  "homepage": "https://github.com/Born-Digital-US/carapace-defaults",
  "support": {
    "issues": "https://github.com/Born-Digital-US/carapace-defaults/issues"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "require": {
    "islandora/carapace": "*",
    "drupal/addtoany": "^1.9",
    "drupal/at_tools": "^3.2",
    "drupal/block_class": "^1.2",
    "drupal/collapse_text": "^1.0",
    "drupal/condition_query": "^1.2",
    "drupal/conditional_fields": "^1.0@alpha",
    "drupal/ds": "^3.8",
    "drupal/easy_breadcrumb": "^1.13",
    "drupal/eva": "^2.0",
    "drupal/facets": "^1.3",
    "drupal/file_download_link": "^1.0",
    "drupal/fixed_block_content": "1.1",
    "drupal/form_options_attributes": "^1.2",
    "drupal/formatter_suite": "^1.2",
    "drupal/masonry_views": "^1.0",
    "drupal/term_condition": "^1.2",
    "drupal/title_field_for_manage_display": "^1.12",
    "drupal/twig_tweak": "^2.6",
    "drupal/views_conditional": "^1.0",
    "drupal/context_groups": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "squizlabs/php_codesniffer": "2.7.1",
    "drupal/coder": "*",
    "sebastian/phpcpd": "*"
  },
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Islandora Foundation",
      "email": "community@islandora.ca",
      "role": "Owner"
    },
    {
      "name": "Hertzel Armengol",
      "email": "hertzel@born-digital.com",
      "role": "Author"
    }
  ]
}
